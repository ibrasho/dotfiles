# Explicitly configured $PATH
PATH="/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"

PATH="/usr/local/sbin:$PATH"
PATH="/usr/local:$PATH"

PATH="/usr/local/opt/coreutils/libexec/gnubin/:$PATH"
MANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"

PATH="$HOME/bin:$PATH"

# Go
PATH="$GOPATH/bin:$PATH"

# Rust Cargo
PATH="$HOME/.cargo/bin:$PATH"

# Composer
# PATH="./vendor/bin:$HOME/.composer/bin:$HOME/.composer/vendor/bin:$PATH"

# NodeJS
PATH="node_modules/.bin:$HOME/.node/bin:$PATH"

# Python
# PATH="$HOME/Library/Python/2.7/bin/:$PATH"

# Android
# PATH="/Development/android-sdk/platform-tools:/Development/android-sdk/tools:$PATH"

# Composer
PATH="vendor/bin:$HOME/.composer/vendor/bin:$PATH"

# added by Anaconda3 5.2.0 installer
PATH="/anaconda3/bin:$PATH"

# Google Cloud SDK.
PATH="$HOME/Tools/google-cloud-sdk/bin/:$PATH"

export PATH MANPATH
